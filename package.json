{
  "name": "klickpages-header",
  "version": "0.1.0",
  "private": true,
  "main": "dist/klickpages-header.umd.js",
  "module": "dist/klickpages-header.esm.js",
  "unpkg": "dist/klickpages-header.min.js",
  "browser": {
    "./sfc": "src/klickpages-header.vue"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/klickpages-header.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/klickpages-header.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/klickpages-header.min.js",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "js-cookie": "^2.2.1",
    "vue": "^2.6.11",
    "vue-i18n": "^8.15.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-unit-jest": "^4.2.2",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "^1.0.0-beta.31",
    "axios-mock-adapter": "^1.17.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "rollup": "^1.31.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-vue": "^5.1.6",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
